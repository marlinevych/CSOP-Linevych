{
  "compilerOptions": {
    "target": "es6",
    "lib": ["ES2015", "ESNext", "DOM"],
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "js"
  },
  "include":["./**/*.ts"],
  "exclude":["wwwroot","node_modules"]
}
